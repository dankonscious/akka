          dataLayer.push({ 'event': 'event_subscribe', 'email': email });
          alert("Thank you for Subscribing.");
          emailInput.value = '';
      } else {
          alert("Please enter a valid email.");
      }
  }

  function handleKeyPress(event) {
      if (event.key === 'Enter') {
          submitEmail();
      }
  }
