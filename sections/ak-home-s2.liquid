<div class="dynamic-sticky parallax-animate ak-px-[20px]">
  <div class="ak-max-w-[var(--page-width)] ak-px-0 ak-py-[100px] ak-mx-auto max-[1024px]:ak-px-[0] max-[1024px]:ak-pb-[16px] max-[1024px]:ak-pt-[32px]">
    
    <div class="ak-overflow-hidden">
      <h2 class="element slideupfade ak-text-[#2E3666] font-['syne'] ak-text-[70px] ak-font-normal ak-leading-[1] ak-tracking-[1.4px] ak-capitalize max-[1024px]:ak-text-[40px]">
        {{ section.settings.headline }}<span class="font-['Blisstwin'] ak-ml-4 ak-capitalize">{{ section.settings.headline_italic }}</span>
      </h2>
    </div>
    <div class="ak-overflow-hidden ak-pb-[20px]">
      <h3 class="element slideupfade ak-text-[#2E3666] font-['syne'] ak-text-[40px] ak-font-normal ak-leading-[1] ak-tracking-[0.8px] ak-capitalize max-[1024px]:ak-text-[24px] max-[1024px]:ak-mt-[16px]">
        {{ section.settings.subheadline }}
      </h3>
    </div>

    <div class="ak-flex ak-mt-[50px] max-[1024px]:ak-flex-col max-[1024px]:ak-mt-[39px]">
      <div class="ak-w-1/2 element scaleup ak-relative max-[1024px]:ak-w-full">
        <div class="ak-pb-[90%] ak-relative ak-overflow-hidden [&:hover_img]:ak-translate-y-[100%] mobile-continue-animation yaxis">
          <img class="ak-block ak-transform ak-transition-transform ak-absolute ak-bottom-[99.9%] ak-left-0 ak-w-full ak-h-full ak-object-cover" loading="eager" src="{{ section.settings.image2 | img_url: 'master' }}">
          <img class="ak-block ak-transform ak-transition-transform ak-absolute ak-top-0 ak-left-0 ak-w-full ak-h-full ak-object-cover" loading="eager" src="{{ section.settings.image1 | img_url: 'master' }}">
        </div>

        <div class="ak-absolute ak-bottom-0 ak-left-0 ak-w-full ak-p-[40px] ak-pr-[200px] max-[1024px]:ak-p-[16px] max-[1024px]:ak-pr-0">
          <svg class="ak-mb-[20px]" xmlns="http://www.w3.org/2000/svg" width="68" height="68" viewBox="0 0 68 68" fill="none">
            <circle cx="34" cy="34" r="33.5" stroke="white"/>
            <path d="M24.9921 36.83C23.6621 36.83 22.8221 35.71 23.0321 34.38C23.2421 33.05 24.5021 32 25.8321 32H42.6321C43.9621 32 44.8721 33.05 44.6621 34.38C44.4521 35.71 43.1221 36.83 41.7921 36.83H24.9921Z" fill="white"/>
          </svg>
          <h3 class="ak-text-white font-['blisstwin'] ak-text-[70px] ak-font-normal ak-leading-[1] ak-tracking-[1.4px] ak-capitalize max-[1024px]:ak-text-[40px]">
            {{ section.settings.text1 }}
          </h3>
        </div>
      </div>
      <div class="ak-w-1/2 element scaleup max-[1024px]:ak-w-full">
        <div class="ak-pb-[90%] ak-relative ak-overflow-hidden [&:hover_img]:ak-translate-x-[100%] mobile-continue-animation xaxis">
          <img class="ak-block ak-transform ak-transition-transform ak-absolute ak-top-0 ak-right-[99.9%] ak-w-full ak-h-full ak-object-cover" loading="eager" src="{{ section.settings.image4 | img_url: 'master' }}">
          <img class="ak-block ak-transform ak-transition-transform ak-absolute ak-top-0 ak-left-0 ak-w-full ak-h-full ak-object-cover" loading="eager" src="{{ section.settings.image3 | img_url: 'master' }}">
        </div>
        <div class="ak-absolute ak-bottom-0 ak-left-0 ak-w-full ak-p-[40px] ak-pr-[200px] max-[1024px]:ak-p-[16px] max-[1024px]:ak-pr-[100px]">
          <svg class="ak-mb-[20px]" xmlns="http://www.w3.org/2000/svg" width="68" height="68" viewBox="0 0 68 68" fill="none">
            <circle cx="34" cy="34" r="33.5" stroke="white"/>
            <path d="M20.4195 35.66C19.5095 35.66 18.8795 34.89 19.0195 33.98C19.1595 33.07 20.1395 32.3 21.0495 32.3H32.6695L34.6995 20.68C34.8395 19.77 35.7495 19 36.6595 19C37.5695 19 38.1995 19.77 38.0595 20.68L36.0295 32.3H47.5795C48.4895 32.3 49.1195 33.07 48.9795 33.98C48.8395 34.89 47.9295 35.66 47.0195 35.66H35.3995L33.3695 47.28C33.2295 48.19 32.3195 48.96 31.4095 48.96C30.4995 48.96 29.8695 48.19 30.0095 47.28L32.0395 35.66H20.4195Z" fill="white"/>
          </svg>
          <h3 class="ak-text-white font-['blisstwin'] ak-text-[70px] ak-font-normal ak-leading-[1] ak-tracking-[1.4px] ak-capitalize max-[1024px]:ak-text-[40px]">
            {{ section.settings.text2 }}
          </h3>
        </div>
      </div>
    </div>
  </div>
</div>



{%  style  %} 
@media screen and (max-width:1200px) {
  .mobile-continue-animation.yaxis img {
    animation: moveImagey 10s infinite;
  }
  .mobile-continue-animation.xaxis img {
    animation: moveImagex 10s infinite;
  }

  @keyframes moveImagey {
    0% {
      transform: translateY(0);
    }
    30% {
      transform: translateY(105%);
    }
    40% {
      transform: translateY(100%);
    }
    50% {
      transform: translateY(100%);
    }
    80% {
      transform: translateY(-5%);
    }
    90% {
      transform: translateY(0);
    }
    100% {
      transform: translateY(0);
    }
  }
  @keyframes moveImagex {
    0% {
      transform: translateX(0);
    }
    30% {
      transform: translateX(105%);
    }
    40% {
      transform: translateX(100%);
    }
    50% {
      transform: translateX(100%);
    }
    80% {
      transform: translateX(-5%);
    }
    90% {
      transform: translateX(0);
    }
    100% {
      transform: translateX(0);
    }
  }
}
{% endstyle %}

