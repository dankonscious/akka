<script>
  var swiper = new Swiper(".swiper-{{ section.id }}", {
    
    spaceBetween: 30,
    centeredSlides: true,    
    loop: true,
    autoplay: true,
    speed: 800,
    simulateTouch: true,
    allowTouchMove: true, // Disables swiping with touch/mouse
    noSwiping: false, // Prevents swiping on specific elements
    preventInteractionOnTransition:false,
    pagination: {
      el: ".swiper-pagination",
      clickable: true,
    },
    navigation: {
      nextEl: ".swiper-button-next",
      prevEl: ".swiper-button-prev",
    },
    breakpoints: {
      1200: {
        simulateTouch: false,
        allowTouchMove: false, // Disables swiping with touch/mouse
        noSwiping: true, // Prevents swiping on specific elements
      }
    },
    on: {
        slideChangeTransitionStart: function () {
            // This event is triggered when the slide starts changing
            const slides = document.querySelectorAll('.swiper-{{ section.id }} .swiper-slide');
            
            // Reset the transform for all slides to normal
            slides.forEach(slide => {
                slide.style.transform = 'translateX(-200px)';
            });
        },
        slideChangeTransitionEnd: function () {
            // This event is triggered when the slide finishes changing
            const slides = document.querySelectorAll('.swiper-{{ section.id }} .swiper-slide');

            // Reset the transform for all slides to normal
            slides.forEach(slide => {
                slide.style.transform = 'translateX(0px)';
            });
        }
    }
  });
</script>

{% style %} 
  #section-{{ section.id }} .swiper-slide {
      transition: transform 1.5s ease-in-out; /* Smooth transition for the movement */
  }
  #section-{{ section.id }} .swiper-wrapper{
      transition-timing-function : linear;
  }
  #section-{{ section.id }} .swiper-button-next:after, 
  #section-{{ section.id }} .swiper-rtl .swiper-button-prev:after , 
  #section-{{ section.id }} .swiper-button-prev:after, 
  #section-{{ section.id }} .swiper-rtl .swiper-button-next:after {
      content:unset;
  }
  #section-{{ section.id }} .swiper-button-next, #section-{{ section.id }} .swiper-button-prev {
      width: 50%;
  }
  #section-{{ section.id }} .swiper-button-next {
      right: -20px;
  }
  #section-{{ section.id }} .swiper-button-prev {
      left: -20px;
  }
{% endstyle %}

