        "label": "Desktop Image 1"
      },
      {
        "type": "image_picker",
        "id": "desktop_image_2",
        "label": "Desktop Image 2"
      },
      {
        "type": "image_picker",
        "id": "desktop_image_3",
        "label": "Desktop Image 3"
      },
      {
        "type": "image_picker",
        "id": "desktop_image_4",
        "label": "Desktop Image 4"
      },
      {
        "type": "image_picker",
        "id": "desktop_image_5",
        "label": "Desktop Image 5"
      },
      {
        "type": "image_picker",
        "id": "desktop_image_6",
        "label": "Desktop Image 6"
      },
      {
        "type": "image_picker",
        "id": "mobile_image_1",
        "label": "Mobile Image 1"
      },
      {
        "type": "image_picker",
        "id": "mobile_image_2",
