<section id="section-1" class="handlebars-template ak-w-full">
    
      <script id="template-desktop" type="text/x-handlebars-template">
        {%- raw -%}{{#unless (isMobile)}}{% endraw %}
          <div id="section-1-desktop" class="ak-relative ak-bg-[#F4D9CC] parallax-animate" data-onload="500">
            <div class="ak-flex ak-mx-auto ak-justify-end ak-items-end ak-gap-[90px] max-[1350px]:ak-gap-[20px]">
              <div class="ak-sticky ak-bottom-[200px] ak-flex ak-justify-end ak-z-[2]">
                <div class="ak-absolute ak-top-[-280px] ak-left-[-300px] parallax-image" data-speed="0.05" data-direction="up">
                  {% if section.settings.desktop_image_1 %}
                    <img loading="lazy" class="element scaleup ak-object-contain ak-max-w-full ak-aspect-[2.07] ak-w-[509px]" src="{{ section.settings.desktop_image_1 | img_url: 'original' }}" alt="" />
                  {% endif %}
                </div>
                <div class="ak-max-w-[590px] ak-mb-[40px] min-[1500px]:ak-mr-[150px] max-[1400px]:ak-ml-[40px] ak-font-['syne']">
                  <div class="ak-overflow-hidden">
                    <h1 class="ak-text-[70px] ak-font-normal ak-leading-[70px] ak-tracking-[1.4px] ak-mb-[45px] ak-capitalize ak-text-[#2E3666] element slideupfade">
                      {{ section.settings.heading }}
                    </h1>
                  </div>
                  <div class="ak-overflow-hidden">
                    <p class="ak-text-[#2E3666] ak-font-['syne'] ak-text-2xl ak-font-normal ak-leading-[1.2] ak-mb-[45px] element slideupfade">
                      {{ section.settings.description }}
                    </p>
                  </div>
                  <div class="ak-overflow-y-hidden ak-pb-9">
                    <a href="{{ section.settings.cta_link }}" class="button element slideupfade ak-flex ak-items-center ak-gap-[16px]" role="button">
                      <span>{{ section.settings.cta_label }}</span>
                      <svg xmlns="http://www.w3.org/2000/svg" width="22" height="34" viewBox="0 0 22 34" fill="none">
                        <path d="M19.3476 23.404C19.3476 23.6817 19.2373 23.948 19.0409 24.1444C18.8445 24.3408 18.5782 24.4511 18.3005 24.4511C18.0227 24.4511 17.7564 24.3408 17.56 24.1444C17.3636 23.948 17.2533 23.6817 17.2533 23.404V12.576L5.08033 24.749C4.88183 24.9339 4.61928 25.0346 4.348 25.0298C4.07672 25.0251 3.81789 24.9152 3.62604 24.7233C3.43418 24.5314 3.32429 24.2726 3.3195 24.0013C3.31471 23.7301 3.41541 23.4675 3.60038 23.269L15.7716 11.0943H4.94362C4.6659 11.0943 4.39956 10.984 4.20318 10.7876C4.00681 10.5912 3.89648 10.3249 3.89648 10.0471C3.89648 9.76942 4.00681 9.50308 4.20318 9.3067C4.39956 9.11032 4.6659 9 4.94362 9H18.3005C18.5782 9 18.8445 9.11032 19.0409 9.3067C19.2373 9.50308 19.3476 9.76942 19.3476 10.0471V23.404Z" fill="#EDEEF5"/>
                      </svg>
                    </a>
                  </div>
                </div>
              </div>
              <div class="ak-flex ak-gap-[300px] ak-flex-col ak-relative ak-z-[5]">
                {% if section.settings.desktop_image_2 %}
                  <div class="ak-mt-[250px]"><img class="element scaleup ak-max-w-[280px] ak-h-auto" src="{{ section.settings.desktop_image_2 | img_url: 'original' }}"></div>
                {% endif %}
                {% if section.settings.desktop_image_3 %}
                  <div><img loading="lazy" class="element scaleup ak-max-w-[280px] ak-h-auto" src="{{ section.settings.desktop_image_3 | img_url: 'original' }}"></div>
                {% endif %}
              </div>
              <div class="ak-sticky ak-bottom-[226px]">
                {% if section.settings.desktop_image_4 %}
                  <div class="parallax-image" data-speed="0.3" data-direction="up"><img class="element scaleup ak-max-w-[280px] ak-h-auto" src="{{ section.settings.desktop_image_4 | img_url: 'original' }}"></div>
                {% endif %}
              </div>
            </div>
            <div class="ak-sticky ak-bottom-[-50px] ak-z-[1]">
              <div class="ak-pt-[20px] ak-flex ak-justify-end ak-relative">
                {% if section.settings.desktop_image_5 %}
                  <div class="ak-absolute ak-top-[-150px] ak-left-0 parallax-image" data-speed="0.2" data-direction="up"><img class="element scaleup ak-max-w-[300px]" loading="lazy" src="{{ section.settings.desktop_image_5 | img_url: 'original' }}"></div>
                {% endif %}
                {% if section.settings.desktop_image_6 %}
                  <div class="ak-relative ak-bottom-[-40px] parallax-image" data-speed="0.05" data-stop="45" data-direction="up"><img class="element scaleup ak-max-w-[1170px]" loading="lazy" src="{{ section.settings.desktop_image_6 | img_url: 'original' }}"></div>
                {% endif %}
              </div>
            </div>
          </div>
        {% raw %}{{/unless}}{% endraw %}
      </script>
    
      <script id="template-mobile" type="text/x-handlebars-template">
        {%- raw -%}{{#if (isMobile)}}{% endraw %}
          <div id="section-1-mobile" class="ak-relative ak-bg-[#F4D9CC] ak-px-[16px] ak-pb-[30px]">
            {% if section.settings.mobile_image_1 %}
              <div><img src="{{ section.settings.mobile_image_1 | img_url: 'original' }}" class="ak-w-full"></div>
            {% endif %}
            <div class="ak-flex ak-items-center ak-justify-center ak-gap-[56px] ak-px-[20px] ak-mt-[-120px] ak-mb-[30px]">
              {% if section.settings.mobile_image_2 %}
                <div class="ak-w-1/2"><img src="{{ section.settings.mobile_image_2 | img_url: 'original' }}" class="ak-w-full"></div>
              {% endif %}
              {% if section.settings.mobile_image_3 %}
                <div class="ak-w-1/2"><img src="{{ section.settings.mobile_image_3 | img_url: 'original' }}" class="ak-w-full"></div>
              {% endif %}
            </div>
            <h2 class="ak-text-[40px] ak-font-normal ak-leading-[1] ak-tracking-[0.8px] ak-mb-[45px] ak-capitalize ak-text-[#2E3666]">
              {{ section.settings.heading }}
            </h2>
            <p class="ak-text-[#2E3666] ak-font-['syne'] ak-text-[20px] ak-font-normal ak-leading-[1.2] ak-mb-[45px]">
              {{ section.settings.description }}
            </p>
            <a href="{{ section.settings.cta_link }}" class="button ak-items-center ak-gap-[16px]" role="button">
              <span>{{ section.settings.cta_label }}</span>
              <svg xmlns="http://www.w3.org/2000/svg" width="22" height="34" viewBox="0 0 22 34" fill="none">
                <path d="M19.3476 23.404C19.3476 23.6817 19.2373 23.948 19.0409 24.1444C18.8445 24.3408 18.5782 24.4511 18.3005 24.4511C18.0227 24.4511 17.7564 24.3408 17.56 24.1444C17.3636 23.948 17.2533 23.6817 17.2533 23.404V12.576L5.08033 24.749C4.88183 24.9339 4.61928 25.0346 4.348 25.0298C4.07672 25.0251 3.81789 24.9152 3.62604 24.7233C3.43418 24.5314 3.32429 24.2726 3.3195 24.0013C3.31471 23.7301 3.41541 23.4675 3.60038 23.269L15.7716 11.0943H4.94362C4.6659 11.0943 4.39956 10.984 4.20318 10.7876C4.00681 10.5912 3.89648 10.3249 3.89648 10.0471C3.89648 9.76942 4.00681 9.50308 4.20318 9.3067C4.39956 9.11032 4.6659 9 4.94362 9H18.3005C18.5782 9 18.8445 9.11032 19.0409 9.3067C19.2373 9.50308 19.3476 9.76942 19.3476 10.0471V23.404Z" fill="#EDEEF5"/>
              </svg>
            </a>
          </div>
        {%- raw -%}{{/if}}{% endraw %}
      </script>
</section>

  
  {% schema %}
  {
    "name": "AK - Home S1",
    "settings": [
      {
        "type": "image_picker",
